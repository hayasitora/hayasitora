	}
	public void setName(String string) {
		// TODO Auto-generated method stub
		
	}
	public void setNo(Integer valueOf) {
		// TODO Auto-generated method stub
		
	}
	public Singer(int nt, String name,int strArray,int strArray2,int strArray3){
		this.nt=nt;
		this.name=name;
		this.kor=strArray;
		this.eng=strArray2;
		this.math=strArray3;
	}
	
}

